{
  "name": "@youngro/feature-youngro-card",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "types": "src/index.tsx",
  "exports": {
    ".": {
      "types": "./src/index.tsx",
      "import": "./src/index.tsx",
      "default": "./src/index.tsx"
    },
    "./dist": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w",
    "dev": "pnpm run build:watch",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "lint": "echo 'no lint for feature-youngro-card yet'"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@youngro/store-card": "workspace:*",
    "@repo/ui": "workspace:*",
    "lucide-react": "^0.546.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0"
  }
}
